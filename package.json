{
  "name": "elements",
  "private": true,
  "repository": "https://github.com/rentalutions/elements.git",
  "author": "Patrick Krawczykowski <patrick@dreadful.design>",
  "scripts": {
    "build": "lerna run build --stream --ignore @rent_avail/docs",
    "build:changed": "lerna run build --since origin/release",
    "build:docs": "yarn workspace @rent_avail/docs build",
    "start": "start-storybook -p 8080 -c .storybook",
    "start:docs": "yarn workspace @rent_avail/docs dev",
    "update": "lerna version --no-push --no-private",
    "test": "jest"
  },
  "devDependencies": {
    "@babel/core": "^7.10.4",
    "@storybook/addon-storysource": "^5.3.19",
    "@storybook/react": "^5.3.19",
    "@testing-library/jest-dom": "^5.11.0",
    "@testing-library/react": "^10.4.4",
    "@testing-library/react-hooks": "^3.3.0",
    "babel-jest": "^26.1.0",
    "babel-loader": "^8.1.0",
    "babel-plugin-styled-components": "^1.10.7",
    "babel-polyfill": "^6.26.0",
    "jest": "^26.1.0",
    "lerna": "^3.22.0",
    "microbundle": "^0.12.2",
    "prettier": "^2.0.2",
    "react-test-renderer": "^16.13.1"
  },
  "dependencies": {
    "clsx": "^1.1.0",
    "framer-motion": "^1.11.1",
    "polished": "^3.6.5",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-feather": "^2.0.4",
    "react-is": "^16.13.1",
    "react-spring": "^8.0.27",
    "styled-components": "^5.1.1",
    "styled-system": "^5.1.5"
  },
  "workspaces": [
    "docs",
    "packages/*"
  ],
  "prettier": {
    "semi": false
  }
}
