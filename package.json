{
  "name": "elements",
  "private": true,
  "repository": "https://github.com/rentalutions/elements.git",
  "author": "Patrick Krawczykowski <patrick@dreadful.design>",
  "scripts": {
    "boot": "yarn add -W lerna && lerna bootstrap --force-local",
    "build": "lerna run build --ignore @rent_avail/docs --stream",
    "build:storybook": "build-storybook --quiet -c .storybook -o storybook",
    "build:docs": "yarn workspace @rent_avail/docs build",
    "check": "lerna changed --include-merged-tags",
    "clean": "lerna clean --yes && rm -rf node_modules",
    "create-package": "node ./scripts/create-package.js",
    "graduate": "lerna version --no-push --no-private --include-merged-tags",
    "release": "lerna publish from-package --yes",
    "start": "concurrently -n storybook,lerna \"yarn storybook\" \"yarn watch\"",
    "start:ts": "concurrently -n lerna,storybook \"yarn watch:ts\" \"yarn storybook:ts\"",
    "start:docs": "rm -rf ./docs/.mdx-data && yarn workspace @rent_avail/docs dev",
    "storybook": "start-storybook --quiet -p 8080 -c .storybook",
    "storybook:ts": "start-storybook --port 8080 --config-dir .storybook-ts --quiet --no-manager-cache",
    "test": "jest",
    "watch": "lerna run watch --parallel",
    "watch:ts": "lerna run watch:ts --parallel"
  },
  "devDependencies": {
    "@babel/core": "^7.20.5",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "@storybook/addon-essentials": "^6.5.15",
    "@storybook/addon-storysource": "^6.5.15",
    "@storybook/react": "^6.5.15",
    "@testing-library/dom": "^8.19.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^12.1.5",
    "@testing-library/react-hooks": "^8.0.1",
    "@testing-library/user-event": "^13.5.0",
    "@types/node": "^16.18.10",
    "@types/react": "^17.0.52",
    "@types/styled-components": "^5.1.26",
    "@types/styled-system": "^5.1.15",
    "@types/styled-system__css": "^5.0.17",
    "@types/styled-system__theme-get": "^5.0.2",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^29.3.1",
    "babel-loader": "^9.1.0",
    "babel-plugin-styled-components": "^2.0.7",
    "concurrently": "^7.6.0",
    "dotenv": "^16.0.3",
    "eslint": "^8.30.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.11",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jest": "^29.3.1",
    "lerna": "^6.1.0",
    "microbundle": "^0.15.1",
    "prettier": "^2.8.1",
    "react": "^18.2.0",
    "react-dom": "^17.0.2",
    "react-is": "^18.2.0",
    "styled-components": "^5.3.6",
    "typescript": "^4.9.4"
  },
  "workspaces": [
    "packages/*"
  ],
  "prettier": {
    "semi": false
  },
  "packageManager": "yarn@3.3.1"
}
