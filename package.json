{
  "name": "elements",
  "private": true,
  "repository": "https://github.com/rentalutions/elements.git",
  "author": "Patrick Krawczykowski <patrick@dreadful.design>",
  "scripts": {
    "build": "lerna run build",
    "build:storybook": "yarn build && storybook build -o storybook",
    "check": "lerna changed --include-merged-tags",
    "clean": "lerna clean --yes && rm -rf node_modules",
    "create-package": "node ./scripts/create-package.js",
    "dev": "concurrently 'yarn:dev:*'",
    "dev:packages": "lerna run watch --stream",
    "dev:storybook": "storybook dev -p 8080 --quiet",
    "graduate": "lerna version --no-push --no-private --include-merged-tags",
    "release": "lerna publish from-package --yes",
    "test": "jest"
  },
  "devDependencies": {
    "@babel/core": "^7.20.12",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "@storybook/addon-actions": "^7.6.16",
    "@storybook/addon-docs": "^7.6.16",
    "@storybook/addon-essentials": "^7.6.16",
    "@storybook/addon-interactions": "^7.6.16",
    "@storybook/addon-links": "^7.6.16",
    "@storybook/addon-mdx-gfm": "^7.6.16",
    "@storybook/cli": "^7.6.16",
    "@storybook/components": "^7.6.16",
    "@storybook/react": "^7.6.16",
    "@storybook/react-webpack5": "^7.6.16",
    "@storybook/testing-library": "^0.2.2",
    "@storybook/theming": "^7.6.16",
    "@testing-library/dom": "^8.20.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/react-hooks": "^8.0.1",
    "@testing-library/user-event": "^14.4.3",
    "@types/jest": "^29.4.0",
    "@types/node": "^18.11.18",
    "@types/react": "^18.0.27",
    "@types/styled-components": "^5.1.26",
    "@types/styled-system": "^5.1.16",
    "@types/styled-system__css": "^5.0.17",
    "@types/styled-system__theme-get": "^5.0.2",
    "babel-loader": "^9.1.2",
    "concurrently": "^7.6.0",
    "dotenv": "^16.0.3",
    "jest": "^29.4.1",
    "jest-environment-jsdom": "^29.4.1",
    "lerna": "^6.4.1",
    "microbundle": "^0.15.1",
    "prettier": "^2.8.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-is": "^18.2.0",
    "shelljs": "^0.8.5",
    "storybook": "^7.6.16",
    "styled-components": "^5.3.6",
    "typescript": "^4.9.5"
  },
  "workspaces": [
    "packages/*"
  ],
  "prettier": {
    "semi": false
  },
  "packageManager": "yarn@3.3.1",
  "resolutions": {
    "@storybook/react-docgen-typescript-plugin": "npm:react-docgen-typescript-plugin@1.0.2",
    "ansi-regex": "npm:ansi-regex@5.0.1",
    "semver@npm:2 || 3 || 4 || 5": "^5.7.2",
    "semver@npm:^6.0.0": "^6.3.1",
    "semver@npm:^7.1.1": "^7.5.3",
    "socks": "^2.7.3"
  }
}
