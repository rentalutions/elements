{
  "license": "MIT",
  "name": "@rent_avail/icons",
  "version": "0.1.7",
  "description": "Icon library for Avail Design System",
  "main": "build/index.js",
  "module": "dist/index.js",
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "scripts": {
    "test": "echo 'run tests from the root directory'",
    "optimize:svg": "svgo -f ./svg --config svgo.config.js",
    "generate:icons": "ts-node --esm ./scripts/build.ts",
    "compile": "yarn optimize:svg && yarn generate:icons",
    "build": "concurrently \"yarn:build:*\"",
    "build:module": "rm -rf dist && babel --root-mode upward src --out-dir dist --copy-files",
    "build:bundle": "rm -rf build && rollup --config rollup.config.js --bundleConfigAsCjs",
    "prepack": "yarn build"
  },
  "publishConfig": {
    "access": "public"
  },
  "contributors": [
    "Patrick Krawczykowski <patrick@dreadful.design>"
  ],
  "peerDependencies": {
    "react": "^17.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.20.7",
    "@types/html-minifier": "^4.0.5",
    "@types/uppercamelcase": "^3.0.2",
    "cheerio": "^1.0.0-rc.12",
    "concurrently": "^7.6.0",
    "feather-icons": "^4.29.0",
    "html-minifier": "^4.0.0",
    "prettier-eslint": "^15.0.1",
    "rollup": "^3.12.1",
    "rollup-plugin-babel": "^4.4.0",
    "svgo": "^3.0.2",
    "ts-node": "^10.9.2",
    "uppercamelcase": "^3.0.0"
  }
}
